<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="?attr/selectableItemBackground"
    android:minHeight="95dp">

    <RatingBar
        android:id="@+id/subject_score_rb_js"
        style="?android:attr/ratingBarStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:isIndicator="true"
        android:numStars="5"
        android:rating="0"
        android:stepSize="0.1"
        android:layout_marginStart="16dp"
        android:layout_marginTop="12dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>
    <TextView
        android:id="@+id/subject_review_nick_name_tc_js"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="섭꽁"
        android:textSize="13sp"
        android:layout_marginTop="3dp"
        app:layout_constraintTop_toBottomOf="@+id/subject_score_rb_js"
        app:layout_constraintStart_toStartOf="@+id/subject_score_rb_js"/>
    <TextView
        android:id="@+id/review_content_tv_js"
        android:layout_width="370dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:ellipsize="end"
        android:text="내용이 들어간다아아아아아아아아아아아아아아아아아아아아아아아아아아아아아아아아아아아아아아아아아아아아아아아아아아"
        android:textColor="@color/black"
        android:textSize="14sp"
        android:maxLines="2"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/subject_review_nick_name_tc_js"
        app:layout_constraintStart_toStartOf="parent" />
    <View
        android:id="@+id/view2"
        android:layout_width="0dp"
        android:layout_height="1dp"
        android:background="#787676"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>